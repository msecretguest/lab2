my_string = "Ф;И;О; О студенте;_Иванов; Иван; Иванович; Студент 3 курса,23 года;_Петров; Семен; Игоревич; Студент 2 " \
            "курса,22 года;_Иванов; Иван; Иванович; Студент 3 курса,23 года; "
k = my_string.split('_')
k1 = ''.join(k[0].split(';'))
k1 = k1.replace('', "", 2)
k2 = ''.join(k[1].split(';'))
k3 = ''.join(k[2].split(';'))
print(k1 + '\n' + k2 + '\n' + k3)
