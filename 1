import re

a: str = input("Введите ЧТО-ТО: ")
for word in a.split():
    word = re.sub(r'[^\w\s]', '', word)
    suffix = 'ов'
    b = word.endswith(suffix)
    if b:
        print(word)

k = input()
k = k.split()
k2 = ""
for i in range(len(k)):
    if k[i].endswith('ов'):
        k2 = k2 + k[i] + ' '
print(k2)
